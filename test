hello=3;
function fibonacci(index,n1,n2)
{
 for(i = 2; index >= i ; i++){
    //somme des deux derniers nombres
    somme = n1 + n2;
    //assigner la dernière valeur à la première
    n1 = n2;
    //attribuer la somme au dernier
    n2 = somme;
 }
 return somme;
}
a=fibonacci(5,0,1);
ba=fibonacci(8,0,1);
